<Defs>

    	<!-- ==================================== M40 Infantry =====================================-->
  
  <ThingDef ParentName="HatMakeableBase">
    <defName>Apparel_3RM40Inf</defName>
    <label>M40 Infantry (3R)</label>
    <description>Standard issued Cap given to Infantrymen</description>
	
    <recipeMaker>
      <researchPrerequisite>ComplexClothing</researchPrerequisite>
    </recipeMaker>
	
    <graphicData>
      <texPath>Things/Apparel/Display/M40Fieldcap_Infantry</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <shaderType>CutoutComplex</shaderType>
    </graphicData>
	
    <costStuffCount>25</costStuffCount>
    <stuffCategories>
      <li>Fabric</li>
      <li>Leathery</li>
    </stuffCategories>
	
    <statBases>
      <WorkToMake>7000</WorkToMake>
      <Mass>0.14</Mass>
      <ArmorRating_Blunt>0.04</ArmorRating_Blunt>
      <ArmorRating_Sharp>0.04</ArmorRating_Sharp>
      <Insulation_Heat>9</Insulation_Heat>
	  <Insulation_Cold>4</Insulation_Cold>
    </statBases>
	
    <equippedStatOffsets>
      <SocialImpact>0.15</SocialImpact>
    </equippedStatOffsets>
	
    <apparel>
      <bodyPartGroups>
        <li>UpperHead</li>
      </bodyPartGroups>
      <wornGraphicPath>Things/Apparel/Hats/M40Fieldcap_Infantry</wornGraphicPath>
      <layers>
        <li>Overhead</li>
      </layers>
	  
      <tags>
       <li>3RInfantry_Hat</li>
      </tags>
	  <useWornGraphicMask>true</useWornGraphicMask>	  
      <defaultOutfitTags>
        <li>Soldier</li>
      </defaultOutfitTags>
    </apparel>

	 <thingCategories>
      <li>3Reich_Hats</li>
    </thingCategories>	
  </ThingDef>
     	
