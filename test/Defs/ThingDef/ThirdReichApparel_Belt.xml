<?xml version="1.0" encoding="utf-8" ?>
<Defs>


           	<!-- ==================================== Harness Assault =====================================-->

	<ThingDef ParentName="ApparelMakeableBase">
    <defName>Apparel_3RHarnessA</defName>
    <label>Assault harness (3R)</label>
    <description>Carries machine gun ammo, usually used by Assault Units.</description>

    <recipeMaker>
      <researchPrerequisite>ComplexClothing</researchPrerequisite>
			<recipeUsers>
				<li>ElectricTailoringBench</li>
				<li>HandTailoringBench</li>
			</recipeUsers>
    </recipeMaker>
	
    <graphicData>
      <texPath>Things/Apparel/Display/HarnessAssault</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <shaderType>CutoutComplex</shaderType>
    </graphicData>
	
    <costStuffCount>40</costStuffCount>
    <stuffCategories>
      <li>Fabric</li>
      <li>Leathery</li>
    </stuffCategories>
	

    <thingCategories>
      <!--<li>Apparel</li>-->
      <li>3Reich_Clothing</li>
    </thingCategories>

	
    <statBases>
	      <Mass>0.20</Mass>
	  <MaxHitPoints>220</MaxHitPoints>
	  <WorkToMake>9000</WorkToMake>
      <ArmorRating_Blunt>0.05</ArmorRating_Blunt>
      <ArmorRating_Sharp>0.06</ArmorRating_Sharp>	 
      <Insulation_Cold>-1</Insulation_Cold>
    </statBases>
	
	
   <equippedStatOffsets>
      <MoveSpeed>-0.01</MoveSpeed>
	  <AimingDelayFactor>-0.09</AimingDelayFactor>
	  <MedicalTendQuality>0.05</MedicalTendQuality>	  
   </equippedStatOffsets>
   

		
		<apparel>
			<bodyPartGroups>
				<li>Torso</li>
				<li>Shoulders</li>
				<li>Waist</li>
			</bodyPartGroups>
			<wornGraphicPath>Things/Apparel/Belt/(3R)HarnessAssault</wornGraphicPath>
			
			<layers>
				<li>Webbing</li>
			</layers>
			
	  <tags>
		<li>3Reich_Harness</li>
      </tags>
	  <useWornGraphicMask>true</useWornGraphicMask>	  
      <defaultOutfitTags>
        <li>Soldier</li>
		<li>Worker</li>
      </defaultOutfitTags>
    </apparel>
	
  </ThingDef>   
  
